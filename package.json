{
  "name": "marketplace",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "migration:create": "npm run typeorm migration:create -- -n InitDatabases",
    "migration-generate": "npm run typeorm:cli -- migration:generate -- -n InitDatabases",
    "seed:all": "cross-env ENTITY=all ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:admin": "cross-env ENTITY=admin ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:currency-config": "cross-env ENTITY=currency-config ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:kms-cmk": "cross-env ENTITY=kms-cmk ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:role": "cross-env ENTITY=role ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:permission": "cross-env ENTITY=permission ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:notification-type": "cross-env ENTITY=notification-type ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:articles": "cross-env ENTITY=articles ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ethereumjs/common": "^2.6.0",
    "@ethereumjs/tx": "^3.5.0",
    "@nestjs-modules/mailer": "^1.3.17",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.3",
    "@openzeppelin/contracts": "^4.7.1",
    "@types/cron": "^1.7.3",
    "@types/lodash": "^4.14.168",
    "@types/parse": "^2.18.16",
    "@types/passport-jwt": "^3.0.4",
    "argon2": "^0.27.1",
    "await-lock": "^2.1.0",
    "aws-sdk": "^2.832.0",
    "bignumber.js": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.6.9",
    "handlebars": "^4.7.7",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.20",
    "mysql2": "^2.3.3",
    "nestjs-typeorm-paginate": "^2.3.1",
    "nodemailer": "^6.7.2",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "socket.io-client": "^4.4.1",
    "swagger-ui-express": "^4.1.6",
    "telegraf": "^4.5.2",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2",
    "web3": "^1.6.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^8.2.6",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
